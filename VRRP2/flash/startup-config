interface Management0
   vrf mgmt
   ip address 192.168.111.5/24
   no lldp transmit
   no lldp receive
!
monitor connectivity
   interval 5
   no shutdown
   interface set int_to_ISP2 Ethernet2
   !
   host DNS
      description "ping do DNS google 8.8.8.8"
      local-interfaces int_to_ISP2 address-only
      ip 8.8.8.8
!
event-handler DNS_google_DOWN
   action bash Cli -p15 -c $'enable\n configure\n interface Ethernet 4\n vrrp 1 priority-level 50\n end'
   !
   trigger on-logging
      regex DNS.*8.8.8.8* is unreachable .*Ethernet2
!
event-handler DNS_google_UP
   action bash Cli -p15 -c $'enable\n configure\n interface Ethernet 4\n vrrp 1 priority-level 150\n end'
   !
   trigger on-logging
      regex DNS.*8.8.8.8* is reachable .*Ethernet2
!
ip routing
ip routing vrf mgmt
!
ip prefix-list LAN_TO_ISP seq 10 permit 192.168.0.0/24
!
ip route vrf mgmt 0.0.0.0/0 192.168.111.250
!
ntp server vrf mgmt tempus1.gum.gov.pl iburst
!
route-map OUTBOND_TO_ISP1 permit 10
   match ip address prefix-list LAN_TO_ISP
!
router bgp 65000
   router-id 22.22.22.22
   neighbor IBGP peer group
   neighbor IBGP remote-as 65000
   neighbor IBGP next-hop-self
   neighbor ISP2 peer group
   neighbor ISP2 remote-as 2222
   neighbor ISP2 route-map OUTBOND_TO_ISP2 out
   neighbor 20.20.20.1 peer group ISP2
   neighbor 192.168.0.1 peer group IBGP
   network 192.168.0.0/24
!
router multicast
   ipv4
      software-forwarding kernel
   !
   ipv6
      software-forwarding kernel
!
